{
  "swagger": "2.0",
  "info": {
    "title": "Journal API",
    "description": "Test Journal API",
    "version": "1.0.0"
  },
  "host": "yazzie-journal-api.onrender.com",
  "basePath": "/",
  "schemes": ["https"],
  "paths": {
    "/users/": {
      "get": {
        "description": "Get all users in DB",
        "parameters": [],
        "responses": {
          "200": {
            "description": "OK"
          },
          "400": {
            "description": "Bad Request"
          }
        }
      },
      "post": {
        "description": "Create new user in DB",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "firstName": {
                  "example": "Han"
                },
                "lastName": {
                  "example": "Solo"
                },
                "email": {
                  "example": "NerfHerder@Corellia.com"
                },
                "phone": {
                  "example": "888-555-5555"
                },
                "notes": {
                  "example": [
                    {
                      "date": "10-10-2022"
                    },
                    {
                      "location": "Provo, UT"
                    },
                    {
                      "image": "Image URL"
                    },
                    {
                      "entry": "Paragraph(s) go here..."
                    },
                    {
                      "tags": ["fun", "party", "chill"]
                    }
                  ]
                }
              }
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Created"
          },
          "412": {
            "description": "Precondition Failed"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/users/{postId}": {
      "get": {
        "description": "Get one user from DB",
        "parameters": [
          {
            "name": "postId",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "400": {
            "description": "Bad Request"
          }
        }
      },
      "put": {
        "description": "Edit one user in DB",
        "parameters": [
          {
            "name": "postId",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "firstName": {
                  "example": "Emperor"
                },
                "lastName": {
                  "example": "Palpatine"
                },
                "email": {
                  "example": "Sith4lyfe@sith.com"
                },
                "phone": {
                  "example": "888-555-5555"
                },
                "notes": {
                  "example": [
                    {
                      "date": "10-10-2022"
                    },
                    {
                      "location": "Provo, UT"
                    },
                    {
                      "image": "Image URL"
                    },
                    {
                      "entry": "Paragraph(s) go here..."
                    },
                    {
                      "tags": ["fun", "party", "chill"]
                    }
                  ]
                }
              }
            }
          }
        ],
        "responses": {
          "204": {
            "description": "No Content"
          },
          "412": {
            "description": "Precondition Failed"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      },
      "delete": {
        "description": "Delete one user from DB",
        "parameters": [
          {
            "name": "postId",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/logins/": {
      "get": {
        "description": "Get all logins from DB",
        "parameters": [],
        "responses": {
          "200": {
            "description": "OK"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      },
      "post": {
        "description": "Create new login in DB",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "username": {
                  "example": "GeneralOrgana"
                },
                "password": {
                  "example": "CoolCloneWarDad"
                }
              }
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Created"
          },
          "412": {
            "description": "Precondition Failed"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/logins/{postId}": {
      "get": {
        "description": "Get one login from DB",
        "parameters": [
          {
            "name": "postId",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      },
      "put": {
        "description": "Edit one login in DB",
        "parameters": [
          {
            "name": "postId",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "username": {
                  "example": "PapaYoda"
                },
                "password": {
                  "example": "AmIGrogusDad?"
                }
              }
            }
          }
        ],
        "responses": {
          "204": {
            "description": "No Content"
          },
          "412": {
            "description": "Precondition Failed"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/logins/{username}": {
      "delete": {
        "description": "Delete one login from DB",
        "parameters": [
          {
            "name": "username",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    }
  }
}
